{{#if error_message}}
	<p>Error Loading Content</p>
	{{error_message}}
{{else}}
	<h3>Citys <a href="/add/city">Add New</a></h3>
	<table class="table table-bordered table-hover">
		<thead>
			<tr>
				<th>City Name</th>
				<th>View</th>
				<th>Edit</th>
			</tr>
		</thead>
		<tbody>
			{{#each city}}
				<tr>
					<td>{{name}}</td>
					<td><a href="/view/city/{{ID}}">View</a></td>
					<td><a href="/edit/city/{{ID}}">Edit</a></td>
				</tr>
			{{/each}}
				<tr>
					<td>New York City</td>
					<td><a href="/view/city/1">View</a></td>
					<td><a href="/edit/city/1">Edit</a></td>
				</tr>
		</tbody>
	</table>
{{/if}}